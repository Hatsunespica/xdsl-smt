// RUN: xdsl-smt.py %s -p=canonicalize-smt -t=smt | filecheck %s

builtin.module() {
    %true: !smt.bool = smt.constant_bool true
    %false: !smt.bool = smt.constant_bool false

    %a: !smt.bool = smt.not %true
    smt.assert %a

    %b: !smt.bool = smt.not %false
    smt.assert %b
}

// CHECK: (assert false)
// CHECK: (assert true)
