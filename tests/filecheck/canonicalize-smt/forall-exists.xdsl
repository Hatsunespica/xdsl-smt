// RUN: xdsl-smt.py %s -p=canonicalize-smt -t=smt | filecheck %s

builtin.module() {
    %b: !smt.bool = smt.forall() {
      ^bb(%x: !smt.bool):
        %y: !smt.bool = smt.constant_bool true
        smt.yield %y
    }
    smt.assert %b

    %c: !smt.bool = smt.forall() {
      ^bb(%x: !smt.bool):
        %y: !smt.bool = smt.constant_bool false
        smt.yield %y
    }
    smt.assert %c

    %d: !smt.bool = smt.exists() {
      ^bb(%x: !smt.bool):
        %y: !smt.bool = smt.constant_bool true
        smt.yield %y
    }
    smt.assert %d

    %e: !smt.bool = smt.exists() {
      ^bb(%x: !smt.bool):
        %y: !smt.bool = smt.constant_bool false
        smt.yield %y
    }
    smt.assert %e

}

// CHECK:      (assert true)
// CHECK-NEXT: (assert false)
// CHECK-NEXT: (assert true)
// CHECK-NEXT: (assert false)