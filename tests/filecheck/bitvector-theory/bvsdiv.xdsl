// RUN: xdsl-smt.py %s | xdsl-smt.py -t=smt | filecheck %s
// RUN: xdsl-smt.py %s -t=smt | z3 -in

builtin.module() {
    %four: !smt.bv.bv<32> = smt.bv.constant !smt.bv.bv_val<4: 32>
    %two: !smt.bv.bv<32> = smt.bv.constant !smt.bv.bv_val<2: 32>
    %div: !smt.bv.bv<32> = smt.bv.sdiv %four, %two
    %eq: !smt.bool = smt.eq %two, %div
    smt.assert %eq
}

// CHECK: (assert (= (_ bv2 32) (bvsdiv (_ bv4 32) (_ bv2 32))))