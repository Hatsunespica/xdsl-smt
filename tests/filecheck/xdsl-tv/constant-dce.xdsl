// RUN: xdsl-smt.py %s -p dce -o %t && xdsl-tv.py %s %t | filecheck %s

builtin.module() {
  func.func() ["sym_name" = "test", "function_type" = !fun<[], [!i32]>, "sym_visibility" = "private"] {
    %x : !i32 = arith.constant() ["value" = 3 : !i32]
    %unused : !i32 = arith.constant() ["value" = 42 : !i32]
    func.return(%x : !i32)
  }
}

// CHECK:      (declare-datatypes ((Pair 2)) ((par (X Y) ((pair (first X) (second Y))))))
// CHECK-NEXT: (define-fun tmp () (Pair Bool (Pair (_ BitVec 32) Bool))
// CHECK-NEXT:   (pair (or (or false false) false) (pair (_ bv3 32) false)))
// CHECK-NEXT: (define-fun tmp_0 () (Pair Bool (Pair (_ BitVec 32) Bool))
// CHECK-NEXT:   (pair (or false false) (pair (_ bv3 32) false)))
// CHECK-NEXT: (assert (=> (first (tmp_0)) (first (tmp))))
